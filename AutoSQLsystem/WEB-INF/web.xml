  <context-param> 
      	<description>Location to store uploaded file</description> 
      	<param-name>file-upload</param-name> 
      	<param-value>
         D:\J2EE\!Tomcat\webapps\AutoSQLsystem\uploadedFiles\
      	</param-value> 
  </context-param>
  <servlet>
   	<servlet-name>Upload</servlet-name>
