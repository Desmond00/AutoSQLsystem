<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>AutoSQLSystem</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>home.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>home</servlet-name>
    <jsp-file>/home.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>home</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <jsp-file>/login.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
    <servlet>
    <servlet-name>signup1</servlet-name>
    <jsp-file>/signUp.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>signup1</servlet-name>
    <url-pattern>/signUp</url-pattern>
  </servlet-mapping>
    <servlet>
    <servlet-name>logout</servlet-name>
    <jsp-file>/logout.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
    <servlet>
    <servlet-name>viewTable</servlet-name>
    <jsp-file>/selectAttribute.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>viewTable</servlet-name>
    <url-pattern>/viewTable</url-pattern>
  </servlet-mapping>
    <servlet>
    <servlet-name>operations</servlet-name>
    <jsp-file>/operations.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>operations</servlet-name>
    <url-pattern>/operations</url-pattern>
  </servlet-mapping>
</web-app>